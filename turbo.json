{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": [
    "**/.env.*local"
  ],
  "pipeline": {
    "generate": {
      "dependsOn": [
        "^generate"
      ],
      "outputs": []
    },
    "lint": {
      "dependsOn": [
        "generate"
      ],
      "outputs": []
    },
    "build": {
      "dependsOn": [
        "generate",
        "lint"
      ],
      "outputs": [
        "dist"
      ]
    },
    "dev": {
      "cache": false,
      "persistent": true
    }
  }
}
